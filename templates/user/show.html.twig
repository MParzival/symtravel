
{% extends 'base.html.twig' %}

{% block body %}
    <div class="row">
        <div class="col-md-8">
            <h2>Destinations déjà visitées : </h2>
            <ul>
                {% for review in user.reviews %}
                    <li>{{ review.destination.name }}</li>
                {% endfor %}
            </ul>
        </div>
        <div class="col-md-4">
            <h2>Nom du compte </h2>
            <p>{{ user.email }}</p>
        </div>
    </div>




{% endblock %}

{% block title %}

{% endblock %}
